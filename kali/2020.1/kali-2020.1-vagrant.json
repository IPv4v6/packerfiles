{
  "builders":
  [
	{
	  "type": "qemu",
	  "accelerator": "kvm",
	  "vm_name": "kali",
	  "iso_url": "https://archive.kali.org/kali-images/kali-2020.1/kali-linux-2020.1-installer-netinst-amd64.iso",
	  "iso_target_path": "./kali-linux-2020.1-installer-netinst-amd64.iso",
	  "iso_checksum": "c9ebf2f7d95cd9349daa3441a25fdfd5e0dab522d03a298dbfe046859c3668db",
	  "iso_checksum_type": "sha256",
	  "output_directory": "builddir",
	  "http_directory": ".",
	  "memory": 768,
	  "disk_size": 20480,
	  "disk_interface": "virtio",
	  "disk_cache": "none",
	  "format": "qcow2",
	  "net_device": "virtio-net",
	  "headless": "true",
	  "ssh_username": "vagrant",
	  "ssh_password": "vagrant",
	  "ssh_port": 22,
	  "ssh_wait_timeout": "30m",
	  "shutdown_command": "sudo -S poweroff",
	  "boot_command": [
		"<esc><wait>",
		"install ",
		"url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed-vagrant.cfg ",
		"locale=en_US keymap=us hostname=kali domain=example.com ",
		"net.ifnames=0",
		"<enter>"
	  ]
	}
  ],
  "provisioners":
  [
    {
	  "type": "shell",
	  "script": "conf.sh",
	  "execute_command": "sudo -S sh '{{.Path}}'"
    }
  ],
  "post-processors":
  [
    {
	  "type": "vagrant",
	  "output": "kali-2020.1-vagrant-libvirt.box"
    }
  ]
}
