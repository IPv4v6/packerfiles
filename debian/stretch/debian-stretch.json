{
  "builders":
  [
	{
	  "type": "qemu",
	  "accelerator": "kvm",
	  "vm_name": "debian-stretch",
	  "iso_url": "http://cdimage.debian.org/cdimage/release/current/amd64/iso-cd/debian-9.9.0-amd64-netinst.iso",
	  "iso_checksum": "d4a22c81c76a66558fb92e690ef70a5d67c685a08216701b15746586520f6e8e",
	  "iso_checksum_type": "sha256",
	  "output_directory": "builddir",
	  "http_directory": ".",
	  "disk_size": 8192,
	  "disk_interface": "virtio",
	  "disk_cache": "none",
	  "format": "raw",
	  "net_device": "virtio-net",
	  "headless": "true",
	  "ssh_username": "vagrant",
	  "ssh_password": "vagrant",
	  "ssh_port": 22,
	  "ssh_wait_timeout": "30m",
	  "shutdown_command": "echo 'vagrant' | sudo -S poweroff",
	  "boot_command": [
		"<esc><wait>",
		"auto ",
		"preseed/url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed.cfg",
		"<enter>"
	  ]
	}
  ],
  "provisioners":
  [
    {
	  "type": "shell",
	  "script": "conf.sh",
	  "execute_command": "echo 'vagrant' | sudo -S sh '{{.Path}}'"
    }
  ]
}
