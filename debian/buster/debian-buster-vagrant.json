{
  "builders":
  [
	{
	  "type": "qemu",
	  "accelerator": "kvm",
	  "vm_name": "debian-buster",
	  "iso_url": "http://cdimage.debian.org/cdimage/release/10.4.0/amd64/iso-cd/debian-10.4.0-amd64-netinst.iso",
	  "iso_target_path": "./debian-10.4.0-amd64-netinst.iso",
	  "iso_checksum": "ab3763d553330e90869487a6843c88f1d4aa199333ff16b653e60e59ac1fc60b",
	  "iso_checksum_type": "sha256",
	  "output_directory": "builddir",
	  "http_directory": ".",
	  "memory": 768,
	  "disk_size": 20480,
	  "disk_interface": "virtio",
	  "disk_cache": "none",
	  "format": "qcow2",
	  "net_device": "virtio-net",
	  "headless": "true",
	  "ssh_username": "vagrant",
	  "ssh_password": "vagrant",
	  "ssh_port": 22,
	  "ssh_wait_timeout": "30m",
	  "shutdown_command": "sudo -S poweroff",
	  "boot_command": [
		"<esc><wait>",
		"auto ",
		"preseed/url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed-vagrant.cfg ",
		"net.ifnames=0",
		"<enter>"
	  ]
	}
  ],
  "provisioners":
  [
    {
	  "type": "shell",
	  "script": "conf.sh",
	  "execute_command": "sudo -S sh '{{.Path}}'"
    }
  ],
  "post-processors":
  [
    {
	  "type": "vagrant",
	  "output": "debian-buster-vagrant-libvirt.box"
    }
  ]
}
